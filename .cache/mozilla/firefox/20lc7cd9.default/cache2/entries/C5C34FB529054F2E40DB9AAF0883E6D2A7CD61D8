function WidgetRemove(cookiesValue, cName) {
//    if (widgetcount > 0) {
//        widgetcount = widgetcount - 1;
//        btnSubmit.disabled = false;
//    }
    $.post("/JQueryAddUserControl.aspx",
			{ ClickedControl: 'widgetremove',
			    CookiesDetail: cookiesValue,
			    CookiesName: cName
			},
			function(data) {
			    //alert(cookiesValue);
			    var getCloseCookiesValue = cookiesValue;
			    //cookies Value -- url + '#;#' + columnIndex + '#;#' + maxRssItems + '#;#' + heightOfBox + '#;#' + no2 + '#;#' + uniqueIdentifier + '#;#' + state
			    var valueSplit = getCloseCookiesValue.split("#;#");

			    //var matchRd = uniqueIdentifier + '#' + maxRssItems + "#" + url ;
			    var matchRd = valueSplit[5] + '#' + valueSplit[2] + "#" + valueSplit[0] + '#' + valueSplit['3'];
			    //alert(matchRd);
			    EnableRadio(matchRd);
			    //alert(matchRd);
			    var enabledCount = 0;
			    for (var x = 0; x < rdWidgetList.length; x++) {
			        if (!rdWidgetList[x].disabled) {
			            enabledCount++;
			        }
			        else {
			            break;
			        }
			    }
			    if (enabledCount == widgetcount) {
			        widgetColumnNo = 1;
			    }
			    MyScreenSize();

			});


}


