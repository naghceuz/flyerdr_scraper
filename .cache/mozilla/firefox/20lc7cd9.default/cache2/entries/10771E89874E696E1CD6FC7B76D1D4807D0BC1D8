function ViewAllStmtWidget(url) {
    url += "?FromWidget=ViewAllStmt";
    window.location.href = url;
}
function ViewStmtWidget(url, fromdate, toDate) {
    url += "?FromWidget=ViewStmt&Fromdate="+fromdate+"&Todate="+toDate;
    window.location.href = url;
}
 function isAlphabetKeyCorpDeals(evt) {

      if (navigator.appName == "Microsoft Internet Explorer") {
          var unicode = evt.charCode ? evt.charCode : evt.keyCode
      }
      else 
      {
          var unicode = evt.charCode ? evt.charCode : evt.KeyAscii
      }
      if(unicode == 32 || unicode ==8 || unicode ==9 || unicode == 45)//alows space,backspace and Tab key
        return true;
      if (unicode < 65 || unicode > 90 && unicode < 97 || unicode > 122) //if not a number
                  return false //disable key press
  

   }

    function isAlphaNumericSpecialCharacterKey(evt) {
      var charCode;
       charCode = (evt.which) ? evt.which : evt.keyCode;
       
        if(charCode == 32 || charCode == 8 || charCode == 9 || charCode == 45 || charCode == 46 || charCode== 40 || charCode ==41 || charCode == 44)//alows space,backspace,Tab key,hypen,FullStop,Open Bracket,Close Bracket,Comma
        {
            return true;
        }
        if ((evt.keyCode ==  35 && evt.charCode ==0)||(evt.keyCode ==  36 && evt.charCode ==0))//home,end keys
        {
        return true;
        }
        if ((evt.keyCode ==  37 && evt.charCode ==0)||(evt.keyCode ==  38 && evt.charCode ==0)
           ||(evt.keyCode ==  39 && evt.charCode ==0)||(evt.keyCode ==  40 && evt.charCode ==0))//arrow keys
        {
        return true;
        }
         if( charCode >= 48 && charCode <= 57)//numeric
            {
              return true;
            }
        
         if((charCode >= 65 && charCode <= 90) || (charCode >= 97 && charCode <= 122))//alphabets
            {
               return true;
            }
       else
         return false;
  }
         
  function AlphabetsOnlyWithWhiteSpaceSingleQuote(e) {


      if (navigator.appName == "Microsoft Internet Explorer") {
          var unicode = e.charCode ? e.charCode : e.keyCode
      }
      else {
          var unicode = e.charCode ? e.charCode : e.KeyAscii
      }
      if (unicode != 8 && unicode != 32 && unicode != 39) { //if the key isn't the backspace key and space key (which we should allow)
          if (unicode != 9) {

              if (unicode < 65 || unicode > 90 && unicode < 97 || unicode > 122) //if not a number
                  return false //disable key press
          }
      }
  }
  function CheckAlphaNumeric(str) {
      var exp = "^[a-zA-Z0-9]*$";
      var regex = new RegExp(exp);
     
      return regex.test(str)
  }

  function CheckPNRLengthWCKIN(str) {

      return ((str.length <= 7) && (str.length >= 6));
  }
